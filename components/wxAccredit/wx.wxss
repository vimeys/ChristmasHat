/* components/wxAccredit/wx.wxss */
.swiper-image{
    width: 100%;
    height: 1000rpx;
}
.test{
    width: 100%;
    height: 100rpx;
    position: fixed;
    top:0;
    left:0;
}
.testCanvas{
    width: 100%;
    height: 500rpx;
}
.wx-bg{
    width: 100%;
    height: 100%;
    position: fixed;
    top:0;
    left: 0;
}
.wx-bg-bgs{
    width: 100%;
    height: 100%;
    background: #000;
    opacity: .1;
    position: relative;
    top: 0;
    left: 0;
    z-index: 1;
}
.wx-pop{
    width: 550rpx;
    height: 400rpx;
    background: #fff;
    border-radius: 10rpx;
    position: absolute;
    top:0;
    bottom: 0;
    right: 0;
    left: 0;
    margin:  auto;
    z-index: 2;
}

